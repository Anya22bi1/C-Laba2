//Задача №50. Игра в пьяницу

#include <iostream>
#include <queue>

int main() {
    std::queue<int> player1, player2; 
    int card;

    for (int i = 0; i < 5; ++i) {
        std::cin >> card;
        player1.push(card);
    }

    for (int i = 0; i < 5; ++i) {
        std::cin >> card;
        player2.push(card);
    }

    long long turns = 0;

    while (!player1.empty() && !player2.empty() && turns < 1000000) {
        int top1 = player1.front();
        player1.pop();
        int top2 = player2.front();
        player2.pop();

        if (top1 == 0 && top2 == 9) {
            player1.push(top1);
            player1.push(top2);
        }
        else if (top2 == 0 && top1 == 9){
            player2.push(top1);
            player2.push(top2);
        }
        else if (top1 > top2) {
            player1.push(top1);
            player1.push(top2);
        }
        else {
            player2.push(top1);
            player2.push(top2);
        }

        turns++;
    }

    if (turns == 1000000) {
        std::cout << "botva";
    }
    else if (player1.empty()) {
        std::cout << "second " << turns;
    }
    else if (player2.empty()){
        std::cout << "first " << turns;
    }

    return 0;
}
